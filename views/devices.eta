<% layout("./layouts.eta") %>

<form action = "/" method = "get">
    <input type = "submit" value = "takaisin" id="back">
</form>

<div id = "devices">
<h3>Devices</h3>

<table>
    <th>
        <tr>
            <th>ID</th><th>Device</th><th>Owner</th>
        </tr>

<% it.devices.forEach((i) =>{%>

    <tr>
        <td><%= i[0] %></td> <td><%= i[1] %></td> <td><%= i[2] %></td>
    </tr>
<%}); %>
</table>
</div>
